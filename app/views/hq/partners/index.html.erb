
    <%- if @partners.empty? %>
      No Partners found
    <%- else %>
      <table>
        <thead>
          <tr>
            <th>
              Osra Num
            </th>
            <th>
              Name
            </th>
            <th>
              Status
            </th>
            <th>
              Start Date
            </th>
            <th>
              Province
            </th>
          </tr>
        </thead>
        <tbody>
          <%- @partners.each do |partner| %>
            <tr>
              <td>
                <%= link_to partner.osra_num, admin_partner_path(partner) %>
              </td>
              <td>
                <%= link_to partner.name, admin_partner_path(partner) %>
              </td>
              <td>
                <%= partner.status.name %>
              </td>
              <td>
                <%= partner.start_date.strftime('%B %d, %Y') %>
              </td>
              <td>
                <%= partner.province.name %>
              </td>
            </tr>
          <%- end %>
        </tbody>
      </table>
    <%- end %>
